				$("select[name=login_type]").val('teacher');
            });
			$('#hrm').click(function () {
                $("input[name=email]").val('hrm@hrm.com');
                $("input[name=password]").val('1234');
				$("select[name=login_type]").val('hrm');
            });
        </script>
		
	<script>
    $('form').submit(function (e) 
	{
        $('#install_progress').show();
        $('#modal_1').show();
        $('.btn').val('Login...');
        $('form').submit();
        e.preventDefault();
    });
</script>

